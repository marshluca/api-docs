%h1 Listing admin_users

%table
  %tr
    %th Name
    %th Email
    %th Ability
    %th Handle

  - @users.each do |user|
    %tr
      %td= user.name
      %td= user.email
      %td= user.ability
      %td #{link_to 'Edit', edit_admin_user_path(user)} | #{link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete}

%br

= link_to 'New User', new_admin_user_path
